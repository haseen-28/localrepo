pipeline{
    agent any 
    
    stages{
        stage("Code"){
            steps{
                echo"cloning the code"
            }
        }
        stage("Build"){
            steps{
                echo"building the code"
            }
        }
        stage("Puch to Docker Hub"){
            steps{
                echo"Puhshing the image to docker hub"
            }
        }
        stage("Deploy"){
            steps{
                echo"Deploying the container"
            }
        }
    }
}
